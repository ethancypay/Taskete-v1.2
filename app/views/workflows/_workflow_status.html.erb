<div class="shadow-md rounded-md py-1 px-2 text-sm flex items-center" id="workflow-status">
  <% if @workflow.completed? %>
    <i class="fas fa-circle t-text-xs text-green-600"></i>
    &nbsp;Completed
  <% else %>
    <i class="fas fa-circle t-text-xs text-indigo-500"></i>
    &nbsp;Running
  <% end %>
</div>
